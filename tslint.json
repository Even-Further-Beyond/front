{
  "extends": ["tslint-react", "tslint:recommended"],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "member-access": false,
    "prefer-for-of": false,
    "jsx-no-lambda": false,
    "interface-name": false,
    "quotemark": [true, "single"],
    "no-bitwise": false,
    "indent": [true, "spaces", 2]
  }
}
